<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_totalTimer">

    <fragment
        android:id="@+id/navigation_totalTimer"
        android:name="com.examtimer1.ui.total_timer.TotalTimerFragment"
        android:label="Total Timer"
        tools:layout="@layout/fragment_total_timer" />

    <fragment
        android:id="@+id/navigation_subjectTimer"
        android:name="com.examtimer1.ui.subject_timer.SubjectTimerFragment"
        android:label="Subject Timer"
        tools:layout="@layout/fragment_subject_timer" />

    <fragment
        android:id="@+id/navigation_record"
        android:name="com.examtimer1.ui.record.RecordFragment"
        android:label="Record"
        tools:layout="@layout/fragment_record" />
</navigation>